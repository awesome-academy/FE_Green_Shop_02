mixin item(img,name,price_left,price_right)
	img(src=img)
	.section_icon1_item1_chung= name
		br
		i.fas.fa-star
		i.fas.fa-star
		i.fas.fa-star
		i.fas.fa-star
		i.fas.fa-star-half
		br
		a.mainprice= price_left
		a.secondprice= price_right

